{# def
    character:Character,
    form:QuartForm,
    join_label:bool = False,
    floating_label:bool = False,
    post_url:str = "",
#}
<PageLayout title={{ character.name }} _attrs={{ attrs }}>

    <div>
        <global.PageTitle>Edit {{ character.full_name }}</global.PageTitle>
    </div>

    <global.FormContainer width="50">
    <character_edit.ProfileForm form={{ form }} join_label={{ join_label }} floating_label={{ floating_label }} post_url={{ post_url }} character={{ character }} />
    </global.FormContainer>

</PageLayout>
